{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mlopsadf1"
		},
		"excelonoutput_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'excelonoutput'"
		},
		"execloninput_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'execloninput'"
		},
		"exinput1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'exinput1'"
		},
		"excelonoutput_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mlopssa.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"excelonoutput_accountKey": {
						"value": "[parameters('excelonoutput_accountKey')]"
					},
					"execloninput_connectionString": {
						"value": "[parameters('execloninput_connectionString')]"
					},
					"exinput1_connectionString": {
						"value": "[parameters('exinput1_connectionString')]"
					},
					"excelonoutput_properties_typeProperties_url": {
						"value": "[parameters('excelonoutput_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}